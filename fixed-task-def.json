{
  "containerDefinitions": [
    {
      "name": "bcos",
      "image": "854428944440.dkr.ecr.us-east-1.amazonaws.com/bcos:staging-b4755cf3aac0733fb1bfe2783088ac92fa187a33",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "name": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "NEXT_PUBLIC_APP_URL",
          "value": "https://staging.bendcare.com"
        },
        {
          "name": "ENVIRONMENT",
          "value": "staging"
        },
        {
          "name": "SKIP_ENV_VALIDATION",
          "value": "true"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:DATABASE_URL::"
        },
        {
          "name": "ANALYTICS_DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:ANALYTICS_DATABASE_URL::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:JWT_SECRET::"
        },
        {
          "name": "JWT_REFRESH_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:JWT_REFRESH_SECRET::"
        },
        {
          "name": "CSRF_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:CSRF_SECRET::"
        },
        {
          "name": "EMAIL_FROM",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:EMAIL_FROM::"
        },
        {
          "name": "ADMIN_NOTIFICATION_EMAILS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:854428944440:secret:staging/bcos-secrets-vDmCm7:ADMIN_NOTIFICATION_EMAILS::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bcos-staging",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:3000/api/health || exit 1"
        ],
        "interval": 30,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 60
      },
      "systemControls": [],
      "readonlyRootFilesystem": false
    }
  ],
  "family": "bcos-staging",
  "taskRoleArn": "arn:aws:iam::854428944440:role/BCOS-ECSTaskRole",
  "executionRoleArn": "arn:aws:iam::854428944440:role/BCOS-ECSTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "8192"
}
